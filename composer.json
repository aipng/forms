	"description": "Reusable form control.",
	"keywords": [
		"form",
		"control",
		"nette"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Karel HorskÃ½",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"AipNg\\": "src/"
		},
		"classmap": [
			"src/Forms/Exceptions"
